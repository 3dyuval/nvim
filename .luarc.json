{
	"runtime.version": "LuaJIT",
	"runtime.path": ["lua/?.lua", "lua/?/init.lua"],
	"diagnostics.globals": [
		"vim",
		"LazyVim",
		"Snacks",
		"it",
		"describe",
		"before_each",
		"after_each"
	],
	"workspace.library": [
		"${3rd}/luv/library",
		"${3rd}/busted/library",
		"${3rd}/luassert/library"
	],
	"workspace.checkThirdParty": false,
	"telemetry.enable": false
}

